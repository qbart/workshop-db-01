<%- 2.times do |i| %>
  <% log_debug "\npluck #{i} {" %>

  <h3>Actors:</h3>
  <table>
    <tbody>
      <% @movie.actors.pluck(:id, :name).each do |id, name| %>
        <tr>
          <td><%= id %></td>
          <td><%= name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <% log_debug "}\n" %>
<% end %>
